{
    "name": "treblig/plivo-php",
    "description": "PHP Package for the Plivo API. Extends the Plivo vendor API library.",
    "type": "library",
    "require": {
        "php": "^7.1",
        "plivo/plivo-php": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "illuminate/support": "^5.5",
        "illuminate/routing": "^5.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Don Gilbert",
            "email": "don.gilbert@mautic.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Treblig\\Plivo\\Laravel\\PlivoServiceProvider"
            ],
            "aliases": {
                "Plivo": "Treblig\\Plivo\\Laravel\\Facade\\Plivo"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Treblig\\Plivo\\": "src/"
        }
    }
}
